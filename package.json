{
  "name": "readme-md",
  "version": "1.3.0",
  "description": "A README.md generator library.",
  "keywords": [
    "commonmark",
    "docs",
    "documentation",
    "generator",
    "markdown",
    "md",
    "readme"
  ],
  "homepage": "https://github.com/jbenner-radham/node-readme-md#readme",
  "bugs": {
    "url": "https://github.com/jbenner-radham/node-readme-md/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbenner-radham/node-readme-md.git"
  },
  "license": "MIT",
  "author": "James Benner <james.benner@gmail.com> (https://www.jamesbenner.com/)",
  "type": "module",
  "exports": {
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.js"
    },
    "import": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "act": "node ./scripts/act.js",
    "prebuild": "yarn clean",
    "build": "node ./scripts/build.js",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky",
    "prepublishOnly": "yarn build",
    "test": "c8 jasmine --config=test/support/jasmine.json"
  },
  "dependencies": {
    "camelcase": "^8.0.0",
    "md-writer": "^1.1.0",
    "type-fest": "^4.27.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/js": "^9.15.0",
    "@types/common-tags": "^1.8.4",
    "@types/jasmine": "^5.1.4",
    "c8": "^10.1.2",
    "chalk": "^5.3.0",
    "command-exists": "^1.2.9",
    "common-tags": "^1.8.2",
    "esbuild": "0.24.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "execa": "^9.5.1",
    "globals": "^15.10.0",
    "husky": "^9.1.7",
    "jasmine": "^5.3.0",
    "lint-staged": "^15.2.10",
    "log-symbols": "^7.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.10",
    "sort-package-json": "^2.11.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.15.0"
  },
  "engines": {
    "node": ">=18.18.0",
    "yarn": "1.x"
  }
}
