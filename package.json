{
  "name": "readme-md",
  "version": "1.4.0",
  "description": "A README.md generator library.",
  "keywords": [
    "commonmark",
    "docs",
    "documentation",
    "generator",
    "markdown",
    "md",
    "readme"
  ],
  "homepage": "https://github.com/jbenner-radham/node-readme-md#readme",
  "bugs": {
    "url": "https://github.com/jbenner-radham/node-readme-md/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbenner-radham/node-readme-md.git"
  },
  "license": "BSD-3-Clause",
  "author": "James Benner <james.benner@gmail.com> (https://www.jamesbenner.com/)",
  "type": "module",
  "exports": {
    "import": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "require": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "act": "node ./scripts/act.js",
    "attw": "attw --ignore-rules false-esm",
    "prebuild": "pnpm run clean",
    "build": "node ./scripts/build.js",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepack": "pnpm run build",
    "prepare": "husky",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "camelcase": "^8.0.0",
    "md-writer": "^1.1.0",
    "type-fest": "^5.0.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@eslint/compat": "^1.2.3",
    "@eslint/js": "^9.15.0",
    "@types/common-tags": "^1.8.4",
    "@vitest/coverage-v8": "^3.2.4",
    "chalk": "^5.3.0",
    "command-exists": "^1.2.9",
    "common-tags": "^1.8.2",
    "esbuild": "0.25.10",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "execa": "^9.5.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "log-symbols": "^7.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sort-package-json": "^3.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.15.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.18.2",
  "engines": {
    "node": ">=20"
  }
}
